{
  "name": "trails",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "electron-dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:3000 && cross-env ELECTRON_START_URL=http://localhost:3000 electron .\"",
    "electron-pack": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.trails.app",
    "productName": "Trails",
    "files": [
      "out/**/*",
      "electron/**/*",
      "package.json"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.15",
    "@ai-sdk/google": "^3.0.10",
    "@ai-sdk/mistral": "^3.0.9",
    "@ai-sdk/openai": "^3.0.12",
    "@xyflow/react": "^12.10.0",
    "ai": "^6.0.39",
    "lucide-react": "^0.562.0",
    "nanoid": "^5.1.6",
    "next": "16.1.3",
    "ollama-ai-provider": "^1.2.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "uuid": "^13.0.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wait-on": "^9.0.3"
  }
}
